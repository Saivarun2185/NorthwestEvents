<body>
	<h2>User Ratings</h2>
	<%if(ratingslist.length > 0){%>
	<%for(let i = 0; i < ratingslist.length; i++){%>
		<label>Username : </label><%=ratingslist[i].username%>
		<br>
		<label>Movie : </label><%=ratingslist[i].movie%>
		<br>
		<label>Rating : </label><%=ratingslist[i].stars%>
		<br>
		<label>Comments : </label><br>
		<textarea><%=ratingslist[i].comments%></textarea>
		<br>
		<br>
		<hr>
	<%}%>
	<%}%>
</body>